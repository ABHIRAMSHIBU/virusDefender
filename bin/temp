#convert binary to hex
import glob
import binascii
def binhex():
    fvirus=open("C:\Users\Naveen\Desktop\virus_test\virus-sample.py","r").read()#open virus database sample
    frand=open("C:\Users\Naveen\Desktop\virus_test\random.py","r").read()# open any file
    print fvirus
    fvirus=fvirus.split("\n")#remove newline
    frand=frand.split("\n")#""
    fvirus=binascii.hexlify(fvirus)#convert binary to hex
    frand=binascii.hexlify(fvirus)#" "
   
    return fvirus,frand
    

def compare():
   f1=[]
   f2=[]
   file1=""
   file2=""
   for i in range(0,len(file1),2):
       f1.append(file1[i:i+2])
           
   for i in range(0,len(file2),2):
       f2.append(file1[i:i+2])
          
   count=0    
   for i in f1:
     for j in f2:
        if i==j:
            count+=1
            if count>=5:
              print"infected"
#   else:
#        print"safezone"
compare()
